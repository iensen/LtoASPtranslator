predicates_keyw = predicates
rules_keyw = rules
colon_hyphen = :-

dot = \.
const = const
in = in
not = not
where = where
comma = ,
lcurparen = \{
rcurparen = \}
bar = \|
lparen = \(
rparen = \)
minus = -
plus = \+
mult = \*
div = /
mod = %
backslash = \\
eq = =
less = <
greater = >
lessoreq = <=
greateroreq = >=
noteq = !=
variable = [A-Z][_a-zA-Z0-9]*
identifier = [_a-z][_a-zA-Z0-9]*
numeral = [0-9]+
spaces = \s+
