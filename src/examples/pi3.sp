rules
%%%%%%%%%% %%%%%%%%%% %%%%%%%%%% %%%%%%%%%% %%%%%%%%%% %%%%%%%%%% %%%%%%%%%% %%%%%%%%%%

p(N) :-
	q((N + 5)),
	#t2(N).

q(N) |
-q(N) :-
	#t1(N).

:-	1 > #count{0, N: q(N)},
	#t1(N).

:-	2 < #count{0, N: q(N)},
	#t1(N).


