% Closed-World Assumption:
-p(AutoVar0) :-
	not p(AutoVar0),
	#universal(AutoVar0).

% Closed-World Assumption:
-q(AutoVar0) :-
	not q(AutoVar0),
	#universal(AutoVar0).

p(N) :-
	q((N + 5)),
	#t2(N).

q(N) |
-q(N) :-
	#t1(N).

:-	1 > #count{0, N: q(N), #t1(N)}.

:-	2 < #count{0, N: q(N), #t1(N)}.


