%%%%%%%%%% %%%%%%%%%% %%%%%%%%%% %%%%%%%%%% %%%%%%%%%% %%%%%%%%%%
sorts
%%%%%%%%%% %%%%%%%%%% %%%%%%%%%% %%%%%%%%%% %%%%%%%%%% %%%%%%%%%%

#t1 = 
	1..3.

#t2 = 
	#t1 + 
	{a, f(b)}.

#t3 = 
	f(#t2, #t1).

#types = 
	#t1 + 
	#t2 + 
	#t3.

#rule_gterms = 
	{a}.

#universal = 
	#types + 
	#rule_gterms.

%%%%%%%%%% %%%%%%%%%% %%%%%%%%%% %%%%%%%%%% %%%%%%%%%% %%%%%%%%%%
predicates
%%%%%%%%%% %%%%%%%%%% %%%%%%%%%% %%%%%%%%%% %%%%%%%%%% %%%%%%%%%%

a2().
p4().
c2().
b2().
p2(#universal).
b1().
g2().
g3().
p3(#universal, #universal, #universal).
f1().
g1().
f3().
p0(#universal, #universal, #universal).
c1().
p1(#universal, #universal, #universal).
a1().
f2().

%%%%%%%%%% %%%%%%%%%% %%%%%%%%%% %%%%%%%%%% %%%%%%%%%% %%%%%%%%%%
rules
%%%%%%%%%% %%%%%%%%%% %%%%%%%%%% %%%%%%%%%% %%%%%%%%%% %%%%%%%%%%

a1.

-a2.

b1 :-
	c1.

b1 :-
	-c2.

b2 :-
	c1.

b2 :-
	-c2.

f1 |
-f2 :-
	g2,
	g1.

f1 |
-f2 :-
	-g3,
	g1.

-f3 :-
	g2,
	g1.

-f3 :-
	-g3,
	g1.

-p0(X1, X2, X3) :-
	p2(X1),
	#t1(X1),
	#t2(X2),
	#t3(X3).

-p0(X1, X2, X3) :-
	p3(a, X2, X3),
	p4,
	#t1(X1),
	#t2(X2),
	#t3(X3).

-p1(X3, X2, X1) :-
	p2(X1),
	#t1(X1),
	#t2(X2),
	#t3(X3).

-p1(X3, X2, X1) :-
	p3(a, X2, X3),
	p4,
	#t1(X1),
	#t2(X2),
	#t3(X3).


